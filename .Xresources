/*URXVT CONFIG*/
URxvt*termName:               rxvt-256color
URxvt*scrollBar:              false
URxvt*perl-lib:               /usr/lib/urxvt/perl/
URxvt*urlLauncher:            chromium
URxvt*matcher.button:         true
URxvt*skipScroll:             true
URxvt*scrollBar_right:        false
URxvt*cursorBlink:            true
URxvt*mouseWheelScrollPage:   false
URxvt*urgentOnBell:           false
URxvt*visualBell:             false
URxvt*saveLines:              10000
URxvt*buffered:               true
URxvt*hold:                   true
URxvt*depth:                  32
URxvt*iso14755:               false
URxvt*iso14755_52:            false
/*FONT CONFIG*/
/*URxvt*font:                   xft:MesloLGS Nerd Font:size=13*/
URxvt*font:                   -uw-ttyp0-medium-r-normal--18-170-75-75-c-90-iso8859-1
URxvt*letterSpace:            0
Xft*antialias:                true
Xft*hinting:                  full
/*COLORS*/
URxvt*background:             [75]#22242E
URxvt*foreground:             #F8F8F2
URxvt*color0:                 #16181F
URxvt*color1:                 #FF5555
URxvt*color2:                 #50FA7B
URxvt*color3:                 #F1FA8C
URxvt*color4:                 #8BE9FD
URxvt*color5:                 #BD93F9
URxvt*color6:                 #8BE9FD
URxvt*color7:                 #F8F8F2
URxvt*color8:                 #16181F
URxvt*color9:                 #FF5555 
URxvt*color10:                #50FA7B
URxvt*color11:                #F1FA8C
URxvt*color12:                #8BE9FD
URxvt*color13:                #BD93F9
URxvt*color14:                #8BE9FD
URxvt*color15:                #F8F8F2
/*URXVT PERLS*/
URxvt.perl-ext-common:        default,clipboard,url-select,keyboard-select,resize-font,ibeamcursor
URxvt.url-select.launcher:    qutebrowser
URxvt.url-select.underline:   true
URxvt.keysym.M-u:             perl:url-select:select_next
URxvt.keysym.M-a:             perl:keyboard-select:activate
URxvt.keysym.M-s:             perl:keyboard-select:search
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.C-Down:          resize-font:smaller
URxvt.keysym.C-Up:            resize-font:bigger
XTerm*cursorBlink:            true
/*xcursor*/
Xcursor.theme:                Vanilla-DMZ
Xcursor.size:                 16
